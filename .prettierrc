{
	"semi": true,
	"useTabs": true,
	"tabWidth": 4,
	"printWidth": 80,
	"singleQuote": true,
	"endOfLine": "lf",
	"trailingComma": "es5",
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"tailwindStylesheet": "src/routes/layout.css"
}
